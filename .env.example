# Gemini LLM配置
GEMINI_API_KEY=your_api_key_here  # 从 Google AI Studio 获取: https://aistudio.google.com/app/apikey
GEMINI_MODEL=gemini-2.0-flash     # 使用的Gemini模型版本

# 视频处理配置
MAX_VIDEO_DURATION=300            # 最大支持视频时长(秒), 超过将被截断
DEFAULT_SAMPLE_RATE=30           # 默认帧采样率, 每N帧提取一帧用于分析
MAX_FRAMES_PER_REQUEST=20        # 单次LLM请求最大帧数, 控制API调用成本

# 文件路径配置
OUTPUT_DIR=output                # 处理后视频的输出目录
TEMP_DIR=temp                    # 临时文件存储目录(帧图片等)

# 日志配置
LOG_LEVEL=INFO                   # 日志级别: DEBUG, INFO, WARNING, ERROR

# 性能配置
MAX_MEMORY_MB=2048              # 最大内存使用限制(MB)
ENABLE_GPU=true                 # 是否启用GPU加速(需要GPU支持)
CONCURRENT_WORKERS=4            # 并发工作线程数